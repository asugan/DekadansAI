{
  "name": "cliproxy-bff-api",
  "version": "1.0.0",
  "description": "Custom Express API wrapper for CLIProxyAPI",
  "main": "dist/server.js",
  "scripts": {
    "auth:generate": "better-auth generate --config src/auth.ts --yes",
    "auth:migrate": "better-auth migrate --config src/auth.ts --yes",
    "build": "tsc -p tsconfig.json",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "lint:ci": "eslint . --ext .ts --max-warnings 0"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "better-auth": "^1.4.18",
    "better-sqlite3": "^12.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "helmet": "^8.1.0"
  },
  "devDependencies": {
    "@better-auth/cli": "^1.4.18",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.4",
    "@types/node": "^22.18.10",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "eslint": "^8.57.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
